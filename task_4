x = float(input())
y = float(input())
def Quarter(x, y):
    if x > 0 and y > 0:
        return "I"
    elif x < 0 and y > 0:
        return "II"
    elif x < 0 and y < 0:
        return "III"
    elif x > 0 and y < 0:
        return "IV"
    elif x == 0 and y == 0:
        return "In Center"
print(Quarter(x, y))
